#include "execute-process-linux"
#include <log-console-defs>

namespace exec
{
	log_console _log_(stdout);
	log_console _err_(stderr);
	
	linux_file_descriptor std_in(STDIN_FILENO);
	linux_file_descriptor std_out(STDOUT_FILENO);
	linux_file_descriptor std_err(STDERR_FILENO);
}

#endif //EXEC_PROCESS_LINUX_EXECUTE_PROCESS_LINUX_DEFS_
